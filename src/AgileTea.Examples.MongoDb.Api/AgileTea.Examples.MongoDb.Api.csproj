    <Nullable>enable</Nullable>
    <UserSecretsId>59b0a8e1-c408-430d-8aac-293a4c3049ff</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
    <DockerfileContext>..\..</DockerfileContext>
